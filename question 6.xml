<?xml version="1.0"?>

<resultset statement="select cat.CAT_ID, cat.CAT_NAME, p.PRO_NAME, sp.SUPP_PRICE from category as cat
inner join product as p on cat.CAT_ID=p.CAT_ID
inner join supplier_pricing as sp on sp.PRO_ID=p.PRO_ID
group by cat.CAT_NAME having min(sp.SUPP_PRICE)
LIMIT 0, 50000
"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<row>
		<field name="CAT_ID">2</field>
		<field name="CAT_NAME">GAMES</field>
		<field name="PRO_NAME">GTA V</field>
		<field name="SUPP_PRICE">1500</field>
	</row>

	<row>
		<field name="CAT_ID">4</field>
		<field name="CAT_NAME">ELECTRONICS</field>
		<field name="PRO_NAME">ROG LAPTOP</field>
		<field name="SUPP_PRICE">30000</field>
	</row>

	<row>
		<field name="CAT_ID">1</field>
		<field name="CAT_NAME">BOOKS</field>
		<field name="PRO_NAME">HARRY POTTER</field>
		<field name="SUPP_PRICE">3000</field>
	</row>

	<row>
		<field name="CAT_ID">5</field>
		<field name="CAT_NAME">CLOTHES</field>
		<field name="PRO_NAME">TSHIRT</field>
		<field name="SUPP_PRICE">2500</field>
	</row>

	<row>
		<field name="CAT_ID">3</field>
		<field name="CAT_NAME">GROCERIES</field>
		<field name="PRO_NAME">OATS</field>
		<field name="SUPP_PRICE">1000</field>
	</row>
</resultset>
